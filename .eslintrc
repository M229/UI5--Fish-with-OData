{
    "root": true,
    "globals": {
        "sap": true,
        "jQuery": true,
        "$": true,
        "URI": true
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes": "warn",
        "semi": ["warn", "always"],
        "comma-dangle": "warn",
        "no-var": "warn",
        "no-warning-comments": "warn"
    }
}
